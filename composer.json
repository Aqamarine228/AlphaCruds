{
    "name": "alpha-dev-team/alphacruds",
    "description": "Helps to automatically create CRUDS in AlphaDev style",
    "keywords": [
        "laravel",
        "alphadev",
        "alpha-dev"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AlphaDevTeam\\AlphaCruds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlphaDevTeam\\AlphaCruds\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aqamarine228",
            "email": "MrFurgJK@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^7.6",
        "squizlabs/php_codesniffer": "^3.7",
        "nwidart/laravel-modules": ">=8.0",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlphaDevTeam\\AlphaCruds\\ServiceProvider"
            ]
        }
    }
}
